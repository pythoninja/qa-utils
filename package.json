{
  "name": "randomicu",
  "version": "0.7.0",
  "description": "Frontend part for random.icu - Free API for QA Engineers",
  "dependencies": {
    "purecss": "^2.0.6"
  },
  "devDependencies": {
    "parcel": "^2.0.0-beta.3.1"
  },
  "scripts": {
    "clean-parcel-cache": "rm -rf ./.parcel-cache",
    "clean-node-cache": "rm -rf ./node_modules",
    "clean": "npm run clean-node-cache && npm run clean-parcel-cache",
    "dev": "parcel watch --dist-dir ./target/classes/static ./src/main/resources/templates/index.html",
    "build": "parcel build --no-source-maps --dist-dir ./target/classes/static ./src/main/resources/templates/index.html",
    "version": "standard-version --tag-prefix '' --sign"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pythoninja/randomicu.git"
  },
  "author": "pythoninja",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pythoninja/randomicu/issues"
  },
  "homepage": "https://random.icu"
}
